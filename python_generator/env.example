# Configuración para el generador de niveles PuzzlePath

# API Key de DeepSeek para generación de niveles con IA
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Configuración de Firebase (opcional, se usa service-account-key.json)
FIREBASE_PROJECT_ID=pathly-68c8a

# Configuración de generación
DEFAULT_DIFFICULTY=normal
DEFAULT_COUNT=1

# Configuración de validación
MAX_RETRIES=3
VALIDATION_TIMEOUT=30

# Configuración de salida
OUTPUT_DIR=./generated_levels 